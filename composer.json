{
    "name": "kodi/application",
    "description": "KODI Yii 2 Web Application",
    "keywords": ["yii2", "kodi"],
    "homepage": "http://meetkodi.com",
    "type": "project",
    "license": "proprietary",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
